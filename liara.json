{
  "django": {
    "pythonVersion": "3.10"
  },
  "app": "kheylisadas",
  "port": 80,
  "build": {
    "location": "iran"
  },
  "disks": [
    {
      "name": "database",
      "mountTo": "database"
    },
    {
      "name": "static",
      "mountTo": "statics"
    },
    {
      "name": "static",
      "mountTo": "static_root"
    },
    {
      "name": "media",
      "mountTo": "media_root"
    },
    {
      "name": "media",
      "mountTo": "medias"
    },
    {
      "name": "uploads",
      "mountTo": "uploads"
    }
  ],
  "deployHook": "python manage.py migrate"
}
